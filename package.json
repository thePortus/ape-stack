    "angular-i18n": "~1.6.6",
    "angular-material": "~1.1.4",
    "angular-material-icons": "~0.7.1",
    "angular-messages": "~1.6.6",
    "angular-sanitize": "~1.6.7",
    "angular-translate": "^2.16.0",
    "angular-translate-handler-log": "~2.16.0",
    "angular-translate-loader-pluggable": "~1.3.1",
    "angular-translate-loader-static-files": "~2.16.0",
    "angular-translate-storage-cookie": "~2.16.0",
    "angular-translate-storage-local": "~2.16.0",
    "angular-ui-grid": "~4.0.7",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "documentation": "^5.3.5",
    "ejs": "~2.5.7",
    "express": "~4.15.2",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "~0.3.2",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "~2.6.1",
    "gulp-documentation": "^3.2.1",
    "gulp-google-webfonts": "~0.0.14",
    "gulp-jshint": "~2.0.4",
    "gulp-less": "~3.3.2",
    "gulp-plumber": "~1.1.0",
    "gulp-rename": "~1.2.2",
    "gulp-shell": "~0.6.3",
    "gulp-sourcemaps": "~2.6.1",
    "gulp-uglify": "~3.0.0",
    "jshint": "~2.9.5",
    "jsonwebtoken": "~8.0.0",
    "less": "~2.7.2",
    "morgan": "~1.8.1",
    "passport": "~0.3.2",
    "passport-jwt": "~3.0.0",
