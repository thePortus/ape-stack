{
  "name": "ape",
  "version": "0.0.0",
  "private": true,
  "description": "Angular, PostgreSQL, ExpressJS (APE) Stack, A Fast Deploying Full-Stack Framework to get your projects off the ground quickly.",
  "keywords": [
    "nodejs",
    "expressjs",
    "angularjs",
    "postgresql",
    "full-stack"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thePortus/ape-stack.git"
  },
  "homepage": "https://github.com/thePortus",
  "author": "David J. Thomas <dave.a.base@gmail.com> (http://thePortus.com)",
  "maintainers": [
    "David J. Thomas <dave.a.base@gmail.com> (http://thePortus.com)"
  ],
  "engines": {
    "node": ">=7.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "bugs": {
    "url": "https://github.com/thePortus/ape-stack/issues",
    "email": "dave.a.base@gmail.com"
  },
  "main": "./bin/www",
  "scripts": {
    "start": "supervisor ./bin/www",
    "postinstall": "gulp && cd ./node_modules/angular-translate-storage-cookie && npm install && cd ../../ && jshint **/*.js --exclude=node_modules/**/*.js",
    "test": "jshint **/*.js --exclude=node_modules/**/*.js && nyc --reporter=html --reporter=text mocha \"./!(client)/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "coveralls": "nyc report --reporter=text-lcov | coveralls mocha \"./!(client)/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "prepublish": "",
    "postpublish": "",
    "setup": "gulp",
    "gather": "gulp setupDev",
    "make": "gulp setupProduction",
    "clean": "gulp clean",
    "reset": "gulp reset",
    "remove": "gulp remove"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.5",
    "angular": "^1.6.5",
    "angular-animate": "^1.6.5",
    "angular-aria": "^1.6.5",
    "angular-dynamic-locale": "^0.1.32",
    "angular-i18n": "^1.6.6",
    "angular-material": "^1.1.4",
    "angular-material-icons": "^0.7.1",
    "angular-messages": "^1.6.6",
    "angular-sanitize": "^1.6.7",
    "angular-translate": "^2.16.0",
    "angular-translate-handler-log": "^2.16.0",
    "angular-translate-loader-pluggable": "^1.3.1",
    "angular-translate-loader-static-files": "^2.16.0",
    "angular-translate-storage-cookie": "^2.16.0",
    "angular-translate-storage-local": "^2.16.0",
    "angular-ui-grid": "^4.0.7",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "^2.5.7",
    "express": "~4.15.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-google-webfonts": "0.0.14",
    "gulp-jshint": "^2.0.4",
    "gulp-less": "^3.3.2",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.3",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-uglifycss": "^1.0.8",
    "jshint": "^2.9.5",
    "jsonwebtoken": "^8.0.0",
    "less": "^2.7.2",
    "morgan": "~1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize-auto": "^0.4.28",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "~2.4.2",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "lru-cache": "^4.1.1",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "process-nextick-args": "^1.0.7",
    "sigmund": "^1.0.1",
    "util-deprecate": "^1.0.2"
  }
