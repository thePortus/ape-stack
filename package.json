{
  "name": "ape",
  "title": "Ape-Stack Server",
  "version": "0.0.0",
  "private": true,
  "description": "Angular, PostgreSQL, ExpressJS (APE) Stack, A Fast Deploying Full-Stack Framework to get your projects off the ground quickly.",
  "keywords": [
    "web",
    "nodejs",
    "expressjs",
    "angular",
    "postgresql",
    "material-design",
    "angular-material",
    "full-stack",
    "scaffold",
    "framework"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thePortus/ape-stack.git"
  },
  "homepage": "https://github.com/thePortus",
  "author": "David J. Thomas <dave.a.base@gmail.com> (http://thePortus.com)",
  "maintainers": [
    "David J. Thomas <dave.a.base@gmail.com> (http://thePortus.com)"
  ],
  "engines": {
    "node": ">=7.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "bugs": {
    "url": "https://github.com/thePortus/ape-stack/issues",
    "email": "dave.a.base@gmail.com"
  },
  "main": "./bin/www",
  "scripts": {
    "start": "supervisor ./bin/www",
    "preinstall": "echo \"Installing npm dependencies...\"",
    "postinstall": "echo \"Dependencies installed successfully!\" && gulp && cd ./node_modules/angular-translate-storage-cookie && npm install",
    "test": "nyc mocha \"./!(client)/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "test_client": "protractor conf.js",
    "test_server": "jshint . --config ./.jshintrc --exclude=node_modules,coverage,client && nyc --reporter=html --reporter=text mocha \"./!(client)/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "test_utils": "mocha \"./utils/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "test_server_utils": "mocha \"./server/utils/**/tests/**/*spec.js\" --recursive --timeout 10000",
    "test_unit": "mocha --recursive --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "",
    "postpublish": "",
    "setup": "gulp",
    "gulp": "gulp",
    "clean": "gulp Clean",
    "remove": "gulp Remove",
    "reset": "gulp Remove && gulp",
    "build_docs": "gulp BuildDocs"
  },
